{
    "name": "Quote-app",
  "description": "Get a Motivation",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "back.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo.png",
